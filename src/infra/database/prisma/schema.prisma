generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("PG_DATABASE_URL")
}

model Hero {
    id String @id @default(uuid())
    name String @unique
    class String
    level Int
    health Int
    mana Int
    createdAt DateTime  @default(now())
    updatedAt DateTime? @updatedAt
}
